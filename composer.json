{
    "name": "withoutthea/laravel-doctrine",
    "description": "The Doctrine 2 implementation that melts with Laravel 5",
    "version": "0.0.1",
    "keywords": ["doctrine", "doctrine 2", "orm", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Loding",
            "email": "nathan@withoutthea.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "4.*|5.*",
        "doctrine/orm": "2.5.*",
        "doctrine/migrations": "1.*"
    },
	"require-dev": {
		"mockery/mockery": "dev-master",
		"phpunit/phpunit": "3.7.*"
	},
    "autoload": {
        "psr-4": {
            "Wota\\LaravelDoctrine\\": "src/",
	        "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
